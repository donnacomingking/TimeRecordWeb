@model TimeRecordModel
@{ ViewData["Title"] = "Delete"; }

<h4 class="mt-3 text-black-50 tr-header">Are you sure you want to delete this time record?</h4>
<a class="mt-4 text-info tr-float-right" asp-action="Index">Back to List</a>
<hr />
<div class="col-sm-6 offset-3 p-3 row tr-box">
    <dl class="col-sm-12 dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.UserId)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.UserId)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.EmployeeName)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.EmployeeName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.ClockIn)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ClockIn)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.ClockOut)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ClockOut)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Active)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Active)
        </dd>
    </dl>

    <form class="col-sm-12" asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-primary" />
    </form>
</div>
